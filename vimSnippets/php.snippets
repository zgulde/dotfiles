

snippet nl "php eol"
PHP_EOL
endsnippet

snippet php "php tags"
<?php $0${1: ?>}
endsnippet

snippet ? "ternary"
(${1:condition}) ? ${2:if_true} : ${3:if_false};
endsnippet

snippet map "array map"
array_map(${2:function(\$${3:element}){$0}}, ${1:array})
endsnippet

snippet filter "array filter"
array_filter(${1:array}, ${2:function(\$${3:element}){$0}})
endsnippet
